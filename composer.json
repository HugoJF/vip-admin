{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "anlutro/l4-settings": "0.4.8",
        "barryvdh/laravel-debugbar": "~2.4",
        "doctrine/dbal": "2.5.x-dev",
        "guzzlehttp/guzzle": "^6.2@dev",
        "imtigger/laravel-settings-ui": "dev-master",
        "invisnik/laravel-steam-auth": "3.*",
        "ixudra/curl": "6.*",
        "kris/laravel-form-builder": "^1.14",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.2.0",
        "rap2hpoutre/laravel-log-viewer": "^0.11.0",
        "sentry/sentry-laravel": "^0.8.0",
        "spatie/laravel-backup": "^3.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^1.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
